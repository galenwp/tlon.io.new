---
import Writing from '../layouts/Base.astro';
const allPosts = await Astro.glob('../pages/posts/*.md');
const pageTitle = 'Tlon Corporation';
---

<Writing>
	<!-- <p class="italic pt-4">2023</p> -->
    <ul class="pt-4">
        {allPosts.map((post) => <li><a href={post.url}>{post.frontmatter.title}</a></li>)}
    </ul>		
</Writing>